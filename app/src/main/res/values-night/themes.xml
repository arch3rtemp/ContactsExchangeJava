<resources>
    <!-- Base application theme. -->
    <style name="Theme.ContactsExchange" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple</item>
        <item name="colorPrimaryVariant">@color/dusky_rose</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/aqua_marine</item>
        <item name="colorSecondaryVariant">@color/shamrock_green</item>
        <item name="colorOnSecondary">@color/black_two</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:windowBackground">@color/background_primary</item>
    </style>
</resources>